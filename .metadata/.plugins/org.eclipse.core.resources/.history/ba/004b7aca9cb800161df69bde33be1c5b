
public class AudioTracks {

		
		private String Title;
		private String ArtistBand;
		int YearOfRelease;
		double Duration;
		private String RecordLabel;
		int StarRating;
		
		
		public void setAudioTracks(String Title, String ArtistBand, int YearOfRelease, double Duration, String Studio, String RecordLabel,int StarRating){
			
			this.Title = Title;
			this.ArtistBand = ArtistBand;
			this.YearOfRelease = YearOfRelease;
			this.Duration = Duration;
			this.RecordLabel = RecordLabel;
			this.StarRating = StarRating;
		}
		public String getTitle(){
			return Title;
		}
		
		public String getArtistBand(){
			return ArtistBand;
		}
		
		public int getYearOfRelease(){
			return YearOfRelease;
		}
		public double getDuration(){
			return Duration;
		}
		
		public String getRecordLabel (){
			return RecordLabel;
		}
		
				
		public int getStarRating(){
			return StarRating;
		}

	}

